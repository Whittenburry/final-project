{{outlet}}
<div class="register-header">
  <h1 class="solofoods">SoloFoods</h1>
</div>
<div class="register-outer-container">
  <form class="register-form">
    {{paper-input class="register-form__input" label="First Name" value=firstName}}
    {{paper-input class="register-form__input" label="Last Name" value=lastName}}
    {{paper-input class="register-form__input" label="E-mail" type="email" value=email}}
    {{paper-input class="register-form__input" label="Password" type="password" value=password}}

    <button class="register-form__button">Submit</button>
  </form>

  <div class="already-registered">
    <h5 class="already-registered__text">Already registered?</h5>
    {{#link-to 'login'}}<h5 class="already-registered__click-here">Login here</h5>{{/link-to}}
  </div>
</div>
